dl.Acc dd.acc table em { font-style:italic; color:#999;}
dl.Acc dd.acc table tbody tr:hover td { background:#E4F3F8; }

dl.Acc dd.acc .foot { text-align:center;}
dl.Acc dd.acc .foot ul { border-top:#B7C6CC 1px solid; box-shadow: 0px 3px 2px #ddd; -webkit-box-shadow: 0px 3px 2px #ddd; -moz-box-shadow: 0px 3px 2px #ddd; border-radius:0 0 5px 5px; -moz-border-radius:0 0 5px 5px; -webkit-border-radius:0 0 5px 5px;}
dl.Acc dd.acc .foot ul li { height:32px; text-align:left;}
dl.Acc dd.acc .foot ul li a { padding-left:40px; text-decoration:none; line-height:32px; display:block;  background-image:url(../resources/img/back/backBtnIE.jpg); background-position:0 0; background-repeat:repeat-x;}
