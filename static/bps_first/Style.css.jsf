dl.Acc dd.acc .foot ul li a:hover { line-height:32px; display:block;  background-image:url(../resources/img/back/backBtnIEhover.jpg); background-position:0 0; background-repeat:repeat-x; color:#000;}
dl.Acc dd.acc .foot ul li a em {color:#525252; cursor:default;}
dl.Acc dd.acc .foot ul li.new a { padding-left:40px; line-height:32px; display:block;  background-image:url(../resources/img/back/backBtnNew.jpg); background-position:0 0; background-repeat:repeat-x;}
dl.Acc dd.acc .foot ul li.new a:hover { line-height:32px; display:block; text-decoration:none;  background-image:url(../resources/img/back/backBtnNewHover.jpg); background-position:0 0; background-repeat:repeat-x;}

dl.Acc dd.acc .foot ul li.new em.nuevo { background: #FF8000; height: 11px; width: 31px; font-size: 8px; line-height: 11px; color: #FFF; padding:1px 5px; text-align: center; margin-top: 11px; border-radius: 3px; -moz-border-radius: 3px; -webkit-border-radius: 3px; }
dl.Acc dd.btns {padding:0px 0px 0px 0px;}
dl.Acc dd.btns h4 { margin-left:30px;}	
