.block .body button { margin:10px 0 0 0;font-family:Arial, Helvetica, sans-serif; color:#0059B2; font-weight:bold; font-size:12px; border:#DDDDDD 1px solid; width:210px; height:30px; border-radius:5px; -moz-border-radius:5px; -webkit-border-radius:5px; box-shadow: 0px 3px 2px #ddd; -webkit-box-shadow: 0px 3px 2px #ddd; -moz-box-shadow: 0px 3px 2px #ddd; cursor:pointer; 
	background-image:url(../resources/img/back/backBtnIE.jpg); background-position:0 0; background-repeat:repeat-x;
}
.block .body button:hover {
	background-image:url(../resources/img/back/backBtnIEhover.jpg); background-position:0 0; background-repeat:repeat-x;
}

.block .body button span.green { color:#59B200;}
.block .body button span.black { color:#000;}

.block .foot { text-align:center;}
.block .foot ul { border-top:#B7C6CC 1px solid; box-shadow: 0px 3px 2px #ddd; -webkit-box-shadow: 0px 3px 2px #ddd; -moz-box-shadow: 0px 3px 2px #ddd; border-radius:0 0 5px 5px; -moz-border-radius:0 0 5px 5px; -webkit-border-radius:0 0 5px 5px;}
.block .foot ul li { height:32px;}  
.block .foot ul li a { line-height:32px; display:block;  background-image:url(../resources/img/back/backBtnIE.jpg); background-position:0 0; background-repeat:repeat-x;}
.block .foot ul li a:hover { line-height:32px; display:block;  background-image:url(../resources/img/back/backBtnIEhover.jpg); background-position:0 0; background-repeat:repeat-x;}

