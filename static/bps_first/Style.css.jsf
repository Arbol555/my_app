/*Acor*/
dl.Acc {}
dl.Acc dt.acc { height:40px; position:relative; background:#7396B9 url(../resources/img/back/mas-menos.png) 5px 5px no-repeat; padding-left:40px;border-radius:5px; -moz-border-radius:5px; -webkit-border-radius:5px;}
dl.Acc dt.current {background:#7396B9 url(../resources/img/back/mas-menos.png) 5px -53px no-repeat; border-radius:5px 5px 0 0; -moz-border-radius:5px 5px 0 0; -webkit-border-radius:5px 5px 0 0;}
dl.Acc dt.acc a { color:#FFF; font-family: 'open_sanssemibold'; font-weight:normal; font-size:22px; line-height:40px; display:block;}
dl.Acc dt.acc .question { position:absolute; right:10px; top:10px; }
dl.Acc dd.acc { background:#FFF;  padding:10px 20px;}
dl.Acc dd.acc h4 { color:#333; line-height:30px; margin-bottom:10px;}
