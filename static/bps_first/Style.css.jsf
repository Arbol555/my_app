.block .body p { margin-top:10px; font-size:12px; line-height:17px;}
.block .body p.estado { color:#59B200; font-size:14px; font-weight:bold;}
.block .body p.estaNone {color:#FF322C; font-size:14px; font-weight:bold;}
.block.sinMsj .body, .sinMsj .body { height:138px; padding-top:10px;}
.block.sinMsj .body p.estado , .sinMsj .body p.estado { color:#59B200; font-size:14px; margin-top:20px; font-weight:bold;}
