.block { margin-bottom:20px; }
.block .head { height:40px; padding:0 0 0 15px; background:#4B77AD; border-radius:5px 5px 0 0; -moz-border-radius:5px 5px 0 0; -webkit-border-radius:5px 5px 0 0; border-bottom:1px solid #FFF;}
